{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "jsx": "preserve",
    "lib": ["dom", "ES2019"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "sourceMap": true
  },
}